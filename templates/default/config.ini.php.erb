<%- end -%>
<%- if @general.key?('session_save_handler') -%>
session_save_handler = <%= @general['session_save_handler'] %>
<%- end -%>
<%- @general['trusted_hosts'].each do |host| -%>
trusted_hosts[] = "<%= host %>"
<%- end -%>
<%- if @general.key?('proxy_client_headers') -%>
<%- @general['proxy_client_headers'].each do |header| -%>
proxy_client_headers[] = <%= header %>
<%- end -%>
<%- end -%>
<%- if @general.key?('proxy_host_headers') -%>
<%- @general['proxy_host_headers'].each do |header| -%>
proxy_host_headers[] = <%= header %>
<%- end -%>
<%- end -%>

[mail]
<%- @mail.each do |key, value| -%>
